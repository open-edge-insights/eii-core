{
    "SchemaVersion": "0.2",
    "Runtime": "docker",
    "RuntimeOptions": {
        "ContainerImage": "ia_video_ingestion:2.3",
        "Privileged": true,
        "TemporaryVolumes": [
            "/dev:/dev"
        ]
    },
    "DataStore": {
        "Protocol": "DIRECT",
        "DataBuckets": [
            {
                "Name": "EIS/VideoIngestion",
                "Permissions": "RO"
            },
            {
                "Name": "EIS/GlobalEnv",
                "Permissions": "RO"
            },
            {
                "Name": "EIS/Publickeys",
                "Permissions": "RO"
            }
        ]
    }
}