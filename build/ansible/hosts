[targets]
master ansible_host=<host IP> ansible_user=<user> ansible_ssh_pass=<ssh password> ansible_become_pass=<sudo password>

# Eg. given below for the reference
# master ansible_host=10.223.97.101 ansible_user=intel ansible_ssh_pass=intel@123 ansible_become_pass=intel@123


