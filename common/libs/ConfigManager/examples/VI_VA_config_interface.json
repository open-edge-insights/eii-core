{
    "/VideoAnalytics/config": {
        "encoding": {
            "level": 95,
            "type": "jpeg"
        },
        "max_jobs": 20,
        "max_workers": 4,
        "queue_size": 10,
        "udfs": [
            {
                "name": "safety_gear_demo",
                "type": "native",
                "device": "CPU",
                "model_xml": "common/udfs/native/safety_gear_demo/ref/frozen_inference_graph.xml",
                "model_bin": "common/udfs/native/safety_gear_demo/ref/frozen_inference_graph.bin"
            }
        ]
    },
    "/VideoAnalytics/interface":{
        "Subscribe": 
            [
                {
                    "Type": "zmq_ipc",
                    "EndPoint": "/home/sagartp/sock",
                    "AppName": "VideoIngestion",
                    "Topics": [
                        "camera1_stream"
                    ]
                },
                { 	
                    "Name": "Cam1_Results",
                    "Type": "zmq_tcp",
                    "EndPoint": "127.0.0.1:65014",		
                    "AppName":"VideoIngestion1",
                    "Topics": ["*"]
                }
            ],
        "Client":
            [
                {
                    "Name":"VideoAnalytics",
                    "AppName": "echo_service",
                    "Type": "zmq_tcp",						
                    "EndPoint": "127.0.0.1:66010"	
                }
            ]
    },
    "/VideoIngestion/config": {
        "encoding": {
            "level": 95,
            "type": "jpeg"
        },
        "ingestor": {
            "loop_video": "true",
            "pipeline": "./test_videos/Safety_Full_Hat_and_Vest.avi",
            "poll_interval": 0.2,
            "queue_size": 10,
            "type": "opencv"
        },
        "loop_video": true,
        "max_jobs": 20.4,
        "string": "Hello World",
        "max_workers": 4,
        "sw_trigger": {
            "init_state": "running"
        },
        "udfs": [
            {
                "n_left_px": 1000,
                "n_right_px": 1000,
                "n_total_px": 300000,
                "name": "dummy",
                "scale_ratio": 4,
                "training_mode": "false",
                "type": "python"
            }
        ]
    },
    "/VideoIngestion/interface": {
        "Publish":[
                { 	
                    "Name": "default",						 
                    "Type": "zmq_ipc",
                    "EndPoint": "/home/sagartp/sock",				
                    "Topics": ["camera1_stream"], 
                    "AllowedClients": ["*"]
                },   
                {
                    "Name": "default",						 
                    "Type": "zmq_tcp",
                    "EndPoint": "127.0.0.1:66010",				
                    "Topics": ["camera1_stream"], 
                    "AllowedClients": ["abc", "pqr"]
                }                       
            ],
        "Server":[
                { 	
                    "Name": "echo_service",
                    "Type": "zmq_tcp",						
                    "EndPoint": "127.0.0.1:66010",			
                    "AllowedClients": ["*"]					
                }			
            ]
    }
}