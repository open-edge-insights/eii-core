{
    "machine_id": "basler-capture-example",
    "trigger_threads": 2,
    "data_ingestion_manager": {
        "ingestors": {
            "video": {
                "poll_interval": 0.01,
                "streams": {
                    "basler": {
                        "0815-0000": {
                            "exposure": 3250
                        }
                    }
                }
            }
        }
    },
    "triggers": {
        "pcb_trigger": {
            "training_mode": false,
            "n_total_px": 300000,
            "n_left_px": 1000,
            "n_right_px": 1000
        }
    },
    "classification": {
        "mqtt_broker_host": "localhost",
        "mqtt_broker_port": 1883,
        "max_workers": 1,
        "classifiers": {
            "pcbdemo": {
                "trigger": ["pcb_trigger"],
                "config": {
                    "ref_img": "./agent/algo_config/ref_pcbdemo/ref.png",
                    "ref_config_roi": "./agent/algo_config/ref_pcbdemo/roi_2.json",
                    "model_xml": "./agent/algo_config/ref_pcbdemo/model_2.xml",
                    "model_bin": "./agent/algo_config/ref_pcbdemo/model_2.bin"
                }
            }
        }
    }
}
