{
    "machine_id": "tool_2",
    "trigger_threads": 1,
    "queue_size" : 10,
    "data_ingestion_manager": {
        "ingestors": {
            "video_file": {
                "video_file": "./test_videos/classification_vid.avi",
                "encoding": {
                    "type": "jpg",
                    "level": 100
                },
                "img_store_type": "inmemory_persistent",
                "loop_video": true
            }
        }
    },
    "triggers": {
        "bypass_trigger": {
            "training_mode": false
        }
    },
    "classification": {
        "max_workers": 1,
        "classifiers": {
            "classification_sample": {
                "trigger": [
                    "bypass_trigger"
                ],
                "config": {
                    "model_xml": "./algos/algo_config/ref_classification/squeezenet1.1_FP32.xml",
                    "model_bin": "./algos/algo_config/ref_classification/squeezenet1.1_FP32.bin",
                    "labels": "./algos/algo_config/ref_classification/squeezenet1.1.labels",
                    "device": "CPU"
                }
            }
        }
    }
}
