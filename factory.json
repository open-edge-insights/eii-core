{
    "machine_id": "tool_2",
    "trigger_threads": 4,
    "data_ingestion_manager": {
        "ingestors": {
            "video_file": {
		    "video_file": "yumei_trigger.avi"
            }
        }
    },
    "storage": {
        "folder": "~/saved_images/",
        "image_format": {
            "type": "png",
            "config": {
                "compression_level": 3
            }
        }
    },
    "triggers": {
        "key_frame_extractor": {
            "kp_threshold": 3,
            "no_part_in_frame_thresh": 3,
            "ref_imgdir": "./agent/ref/",
            "ref_imgcount": 4
        }
    },
    "classification": {
        "mqtt_broker_host": "localhost",
        "mqtt_broker_port": 1883,
        "max_workers": 1,
        "classifiers": {
            "yumei": {
                "trigger": ["key_frame_extractor"],
                "config": {
                    "ref_imgdir": "./agent/ref/",
                    "ref_imgcount": 4,
                    "ref_config_roi": "./agent/ref/ym_roi.json"
                }
            }
        }
    },
    "database": {
        "host": "localhost",
        "port": 5432,
        "database_name": "ali",
        "username": "ali"
    }
}
